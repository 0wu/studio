{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "target": "esnext",
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strict": true,
    "typeRoots": ["node_modules/@types", "./typings"],
    "paths": {}
  },
  "include": ["app/**/*"],
  "exclude": ["node_modules", "app/players/UserNodePlayer/nodeTransformerWorker/typescript/**/*"]
}
